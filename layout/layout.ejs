<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Smileyan</title>
</head>
 
<body>
    <!-- 网站名 -->
    <h1>
        <a href="/"><%= config.title  %></a> |
        <a href="/">Home</a> | 
        <a href="<%-url_for(config.archive_dir)%>">Archive</a> | 
        <a href="/about/">About</a>
    </h1>
    <%- body %> 

    <h3>categories</h3>
    <ul>
        <% site.categories.each(category=>{ %>
            <li>
                <a href="<%- url_for(category.path) %>">
                    <%- category.name  %> 
                </a>
            </li>
        <% }) %>
    </ul>

    <h3>tags</h3>
    <ul>
        <% site.tags.each(tag=>{ %>
            <li>
                <a href="<%- url_for(tag.path) %>">
                    <%- tag.name  %> 
                </a>
            </li>
        <% }) %>
    </ul>

    

</body>
 
</html>


